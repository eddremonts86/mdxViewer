{
    "name": "mdxviewer",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run server\" \"vite\"",
        "dev:watch": "concurrently \"npm run server\" \"node scripts/devWithWatcher.js\"",
        "server": "tsx server/index.ts",
        "client": "vite",
        "watch:content": "node scripts/contentWatcher.js",
        "generate:index": "node scripts/generateIndex.js",
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "css:clean": "rm -rf node_modules/.vite && vite build",
        "validate:ai": "scripts/sh/validate-ai-rules.sh",
        "pre-commit": "npm run lint && npm run validate:ai"
    },
    "dependencies": {
        "@fontsource/inter": "^5.2.6",
        "@fontsource/jetbrains-mono": "^5.2.6",
        "@mdx-js/react": "^3.1.0",
        "@mdx-js/rollup": "^3.1.0",
        "@radix-ui/react-checkbox": "^1.3.2",
        "@radix-ui/react-dialog": "^1.1.14",
        "@radix-ui/react-dropdown-menu": "^2.1.15",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-toast": "^1.2.14",
        "@tailwindcss/typography": "^0.5.16",
        "@tailwindcss/vite": "^4.1.10",
        "@tanstack/react-query": "^5.81.5",
        "@tanstack/react-query-devtools": "^5.81.5",
        "@types/dom-to-image": "^2.6.7",
        "@types/node": "^24.0.3",
        "@types/react-syntax-highlighter": "^15.5.13",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cors": "^2.8.5",
        "express": "^5.1.0",
        "file-saver": "^2.0.5",
        "gray-matter": "^4.0.3",
        "html2canvas": "^1.4.1",
        "jspdf": "^3.0.1",
        "lucide-react": "^0.522.0",
        "multer": "^2.0.1",
        "node-fetch": "^2.7.0",
        "prismjs": "^1.30.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-markdown": "^10.1.0",
        "react-resizable-panels": "^3.0.3",
        "react-router-dom": "^7.6.2",
        "react-syntax-highlighter": "^15.6.1",
        "react-to-print": "^3.1.0",
        "rehype-katex": "^7.0.1",
        "rehype-raw": "^7.0.0",
        "remark-gfm": "^4.0.1",
        "remark-math": "^6.0.0",
        "shiki": "^3.7.0",
        "tailwind-merge": "^3.3.1",
        "tailwindcss-animate": "^1.0.7",
        "zustand": "^5.0.5"
    },
    "devDependencies": {
        "@eslint/js": "^9.25.0",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/file-saver": "^2.0.7",
        "@types/multer": "^1.4.13",
        "@types/prismjs": "^1.26.5",
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.2",
        "@vitejs/plugin-react": "^4.4.1",
        "autoprefixer": "^10.4.21",
        "chokidar": "^4.0.3",
        "concurrently": "^9.2.0",
        "eslint": "^9.25.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "globals": "^16.0.0",
        "postcss": "^8.5.6",
        "tailwindcss": "^4.1.10",
        "tsx": "^4.20.3",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.30.1",
        "vite": "^6.3.5"
    }
